	Journal = {Procedia Computer Science},
	Keywords = {optimisation},
	Note = {\{ANT\} 2012 and MobiWIS 2012},
	Number = {0},
	Pages = {821 - 826},
	Title = {Analysis of the Co-routing Problem in Agent-based Carpooling Simulation},
	Url = {http://www.sciencedirect.com/science/article/pii/S1877050912004632},
	Volume = {10},
	Year = {2012},
